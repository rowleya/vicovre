<?xml version="1.0" encoding="UTF-8"?>
<permissions>
    <permissionSet className="PermissionSet">
        <role name="Administrator" allow="true">
            <operation>Change Permissions</operation>
        </role>
        <user name="?user" allow="true">
            <operation>Change Permissions</operation>
        </user>
    </permissionSet>
    <permissionSet className="HarvestSource">
        <role name="Administrator" allow="true">
            <operation>Read Harvest Item</operation>
            <operation>Delete Harvest Item</operation>
            <operation>Change Harvest Item</operation>
            <operation>Harvest Manually</operation>
        </role>
        <user name="?user" allow="true">
            <operation>Read Harvest Item</operation>
            <operation>Delete Harvest Item</operation>
            <operation>Change Harvest Item</operation>
            <operation>Harvest Manually</operation>
        </user>
    </permissionSet>
</permissions>