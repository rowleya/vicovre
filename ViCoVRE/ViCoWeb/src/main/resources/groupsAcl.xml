<?xml version="1.0" encoding="UTF-8"?>
<permissions>
    <!-- Who can see/alter the groups -->
    <permissionSet>
        <role name="AuthUser" allow="true">
            <operation>Read</operation>
        </role>
        <role name="Administrator" allow="true">
            <operation>Read</operation>
            <operation>Write</operation>
            <operation>Create</operation>
            <operation>Delete</operation>
        </role>
    </permissionSet>
</permissions>